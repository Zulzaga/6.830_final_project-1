package minidb;


public class AVLNode {
 public AVLNode left;
 public AVLNode right;
 public AVLNode parent;
 public int key;
 public int balance;


 public AVLNode(int k, int position) {
  left = right = parent = null;
  balance = 0;
  key = k;
  position = position
 }
 public String toString() {
  return "" + key;
 }

}
